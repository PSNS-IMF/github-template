<package >
  <metadata>
    <id>Psns.Common.Build.GithubTemplate</id>
    <title>PSNS Common Github Template</title>
    <authors>Jeff Purcell</authors>
    <owners>109.14</owners>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <description>Contains template files to start a new Github-hosted project.</description>
    <projectUrl>https://github.com/PSNS-IMF/github-template</projectUrl>
